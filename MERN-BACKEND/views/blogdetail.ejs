      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
    />
  </head>
  <body>
    <nav>
      <div class="menu-icon" data-aos="fade-down-right" data-aos-delay="400">
         <a href="/home" style="z-index: 160;"><span class="material-symbols-outlined"> menu </span></a> 
      </div>
      <div class="logo">
          <h3><a href="merlin.html" class="logo-name">Anima</a></h3>
      </div>
      <ul class="nav-links">
          <li><a class="nav-content" href="/projects">Projects</a></li>
          <li><a class="nav-content" href="/Members">Members</a></li>
          <li><a class="nav-content" href="/Events">Events</a></li>
          <div class="logo-box" data-aos="zoom-in">
              <img src="/images/drone-svgrepo-com.svg" class="logo logo-1" />
              <img src="/images/fighter-jet-plane-svgrepo-com.svg" class="logo logo-2" />
              <img src="/images/ANIMA-logo.jpg" class="logo logo-3" />
          </div>
          <li><a class="nav-content" href="/blogs">Blogs</a></li>
          <li><a class="nav-content" href="/contact">Gallery</a></li>
          <li><a class="nav-content" href="/users/logout" id="login">About Us</a></li>
      </ul>
      <div class="burger">
          <div class="line1"></div>
          <div class="line2"></div>
          <div class="line3"></div>
      </div>
      <div class="login-icon" data-aos="fade-down-left" data-aos-delay="400">
          <span class="material-symbols-outlined"> face </span>
      </div>
      <hr class="navbar-line">
  </nav>
    <div class="heading">Blogs</div>
    <div class="blog-boxes">
      <div class="blog-list blog-box">
        <h1 class="list-head">Blog-list</h1>
        <ol>
          <% products.forEach(function(blog) { %> 
            <li class="list-item"> <a href="/blogmiddle/<%= blog._id %>" style="text-decoration: none;" >  <%= blog.blogname %> </a> </li>
               <% }); %>
        </ol>
      </div>
      <div class="blog-content blog-box">
        <h1><%= bloggg.blogname %></h1>
        <p class="para para-1">
          <%= bloggg.para1 %>
        </p>
        <div class="image-box">
          <img src="data:image/jpeg;base64,<%= bloggg.image1.toString('base64') %>" >
        </div>

        <p class="para para-2">
          <%= bloggg.para2 %>
        </p>
        <div class="image-box">
          <img src="data:image/jpeg;base64,<%= bloggg.image2.toString('base64') %>" >
        </div>
        <p class="para para-3">
          <%= bloggg.para3 %>
        </p>
      </div>
    </div>
    <script>
   
      const backendUrl = 'https://your-backend-service.onrender.com';

      // Example API call using the backend URL
      fetch(`${backendUrl}/api/users`)
          .then(response => response.json())
          .then(data => console.log(data))
          .catch(error => console.error('Error:', error));
  </script>

  </body>
  <script src="script.js"></script>
</html>
